    "gen": "node --loader ts-node/esm  --experimental-modules --experimental-specifier-resolution=node src/gen/dom.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/vite-plugin": "^3.1.2",
    "sass": "^1.45.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.0"
  },
  "module": "./dist/tuff.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tuff.es.js"
    }
  }
}
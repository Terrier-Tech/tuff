{
  "name": "tuff-core",
  "type": "module",
  "files": [
    "*"
  ],
  "version": "0.21.06",
  "repository": {
    "type": "git",
    "url": "https://github.com/Terrier-Tech/tuff"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepub": "tsc && vite build && npm-prepare-dist -s prepub -s pub -s dev -s gen -s docs",
    "pub": "npm publish ./dist",
    "tsc": "tsc",
    "gen": "node --loader ts-node/esm  --experimental-modules --experimental-specifier-resolution=node src/gen/dom.ts",
    "test": "vitest",
    "typedoc": "typedoc --plugin typedoc-plugin-extras --footerDate"
  },
  "devDependencies": {
    "@types/node": "^17.0.30",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "npm-prepare-dist": "^0.4.0",
    "rollup-plugin-folder-input": "^1.0.1",
    "sass": "^1.51.0",
    "ts-node": "^10.7.0",
    "typedoc": "^0.23.16",
    "typedoc-plugin-extras": "^2.3.0",
    "typescript": "4.6",
    "vite": "^3.2.5",
    "vitest": "^0.27.0"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./keyboard": {
      "import": "./dist/keyboard.js"
    },
    "./state": {
      "import": "./dist/state.js"
    },
    "./strings": {
      "import": "./dist/strings.js"
    },
    "./sets": {
      "import": "./dist/sets.js"
    },
    "./urls": {
      "import": "./dist/urls.js"
    },
    "./objects": {
      "import": "./dist/objects.js"
    },
    "./parts": {
      "import": "./dist/parts.js"
    },
    "./messages": {
      "import": "./dist/messages.js"
    },
    "./nav": {
      "import": "./dist/nav.js"
    },
    "./routing": {
      "import": "./dist/routing.js"
    },
    "./svg": {
      "import": "./dist/svg.js"
    },
    "./mat": {
      "import": "./dist/mat.js"
    },
    "./forms": {
      "import": "./dist/forms.js"
    },
    "./types": {
      "import": "./dist/types.js"
    },
    "./vite-env": {
      "import": "./dist/vite-env.js"
    },
    "./html": {
      "import": "./dist/html.js"
    },
    "./index": {
      "import": "./dist/index.js"
    },
    "./trig": {
      "import": "./dist/trig.js"
    },
    "./box": {
      "import": "./dist/box.js"
    },
    "./vec": {
      "import": "./dist/vec.js"
    },
    "./tags": {
      "import": "./dist/tags.js"
    },
    "./arrays": {
      "import": "./dist/arrays.js"
    },
    "./logging": {
      "import": "./dist/logging.js"
    }
  },
  "dependencies": {
    "@types/path-to-regexp": "^1.7.0",
    "path-to-regexp": "^6.2.0",
    "typesafe-routes": "^7.0.2"
  }
}

{
  "name": "tuff-core",
  "type": "module",
  "files": ["dist"],
  "version": "0.1.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/Terrier-Tech/tuff"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cp -r types/*.d.ts dist",

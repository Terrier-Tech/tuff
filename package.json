{
  "name": "tuff-core",
  "type": "module",
  "files": [
    "*"
  ],
  "version": "0.27.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Terrier-Tech/tuff"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepub": "rm -r dist && tsc && vite build && npm-prepare-dist -s prepub -s pub -s dev -s gen -s typedoc",
    "pub": "npm publish ./dist",
    "tsc": "tsc",
    "gen": "node --loader ts-node/esm  --experimental-modules --experimental-specifier-resolution=node src/gen/dom.ts",
    "test": "vitest",
    "typedoc": "typedoc --plugin typedoc-plugin-extras --footerDate"
  },
  "devDependencies": {
    "@types/node": "^20.2.1",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "npm-prepare-dist": "^0.4.0",
    "rollup-plugin-folder-input": "^1.0.1",
    "sass": "^1.62.1",
    "ts-node": "^10.7.0",
    "typedoc": "^0.24.7",
    "typedoc-plugin-extras": "^2.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.3.8",
    "vitest": "^0.31.1"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./keyboard": {
      "import": "./keyboard.js"
    },
    "./state": {
      "import": "./state.js"
    },
    "./strings": {
      "import": "./strings.js"
    },
    "./sets": {
      "import": "./sets.js"
    },
    "./urls": {
      "import": "./urls.js"
    },
    "./objects": {
      "import": "./objects.js"
    },
    "./parts": {
      "import": "./parts.js"
    },
    "./messages": {
      "import": "./messages.js"
    },
    "./nav": {
      "import": "./nav.js"
    },
    "./routing": {
      "import": "./routing.js"
    },
    "./svg": {
      "import": "./svg.js"
    },
    "./mat": {
      "import": "./mat.js"
    },
    "./forms": {
      "import": "./forms.js"
    },
    "./types": {
      "import": "./types.js"
    },
    "./vite-env": {
      "import": "./vite-env.js"
    },
    "./html": {
      "import": "./html.js"
    },
    "./index": {
      "import": "./index.js"
    },
    "./trig": {
      "import": "./trig.js"
    },
    "./box": {
      "import": "./box.js"
    },
    "./vec": {
      "import": "./vec.js"
    },
    "./tags": {
      "import": "./tags.js"
    },
    "./arrays": {
      "import": "./arrays.js"
    },
    "./logging": {
      "import": "./logging.js"
    },
    "./time": {
      "import": "./time.js"
    }
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0",
    "typesafe-routes": "^10.0.2"
  }
}
